<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javaassignment.myassignment.controller.ManageInventoryController">
    <TableView fx:id="inventoryTableView">
        <columns>
            <TableColumn fx:id="isbnColumn" text="ISBN" />
            <TableColumn fx:id="titleColumn" text="Title" />
            <TableColumn fx:id="authorColumn" text="Author" />
            <TableColumn fx:id="genreColumn" text="Genre" />
            <TableColumn fx:id="priceColumn" text="Price" />
            <TableColumn fx:id="quantityColumn" text="Quantity" />
        </columns>
    </TableView>
</VBox>
